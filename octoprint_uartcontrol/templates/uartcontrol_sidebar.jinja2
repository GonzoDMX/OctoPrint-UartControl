<div class="row-fluid">
  <label>Received Messages: </label>
</div>
<div class="row-fluid">
<select name="Cars" size="5">
    <option value="Merceders"> Merceders </option>
    <option value="BMW"> BMW </option>
    <option value="Jaguar"> Jaguar </option>
    <option value="Lamborghini"> Lamborghini </option>
    <option value="Ferrari"> Ferrari </option>
    <option value="Ford"> Ford </option>
</select>
</div>
<hr class="solid">
<div class="row-fluid" data-bind="foreach: uartButtons">
  <div class="uartcontrol-button-row row-fluid">
    <div class="uartcontrol-button-label span6">
      <i data-bind="class: icon"></i>
      <span data-bind="text: name"></span>
    </div>
    <div class="uartcontrol-button-controls input-prepend input-append span6">
      <button class="btn" data-toggle="dropdown" data-bind="click: $parent.sendUartMessage">SEND</button>
    </div>
  </div>
</div>
